{
  "name": "latest",
  "version": "0.0.2",
  "description": "displays channel from https://ltst.xyz and stores the update in the embedding application",
  "keywords": [
    "latest",
    "dashboard",
    "updates"
  ],
  "license": "MIT",
  "author": "Joseph Wright",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.18",
    "blockprotocol": "^0.1.3"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.6",
    "block-scripts": "0.0.17",
    "mock-block-dock": "0.0.32",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Latest",
    "icon": "public/ltst-block-icon.svg",
    "image": "public/ltst-block-preview.png",
    "name": "Latest",
    "protocol": "0.2"
  }
}
